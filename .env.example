TOWBOOK_USERNAME=your_towbook_username
TOWBOOK_PASSWORD=your_towbook_password

# Authentication
# Set to 'false' to hide authentication UI in production
# NEXT_PUBLIC_ENABLE_AUTH=true

# AWS SES Configuration
# AWS SES region (e.g., us-east-1, us-west-2, eu-west-1)
AWS_SES_REGION=us-east-1

# AWS SES credentials
AWS_SES_ACCESS_KEY_ID=your_access_key_here
AWS_SES_SECRET_ACCESS_KEY=your_secret_access_key_here

# Contact email
NEXT_PUBLIC_CONTACT_EMAIL=web@greensmil.com

# Email addresses
# The verified sender email address in AWS SES for customer confirmations
AWS_SES_FROM_EMAIL=noreply@greensmil.com

# Email address where contact form submissions will be sent (you receive notifications here)
AWS_SES_TO_EMAIL=web@greensmil.com
